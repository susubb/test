<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>OneServlet</servlet-name>
        <servlet-class>servlet.OneServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OneServlet</servlet-name>
        <url-pattern>/one.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TwoServlet</servlet-name>
        <servlet-class>servlet.TwoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TwoServlet</servlet-name>
        <url-pattern>/two.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ThreeServlet</servlet-name>
        <servlet-class>servlet.ThreeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ThreeServlet</servlet-name>
        <url-pattern>/three.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FourServlet</servlet-name>
        <servlet-class>servlet.FourServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FourServlet</servlet-name>
        <url-pattern>/four.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FiveServlet</servlet-name>
        <servlet-class>servlet.FiveServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FiveServlet</servlet-name>
        <url-pattern>/student/save.do</url-pattern>
        <url-pattern>/student/update.do</url-pattern>
        <url-pattern>/student/delete.do</url-pattern>
        <url-pattern>/student/select.do</url-pattern>
    </servlet-mapping>
</web-app>